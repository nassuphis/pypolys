% ---- R macro expansion ----
<<, engine='R',include=FALSE>>=
make_logo<-function(logo,font){
knitr::asis_output(glue::glue(R"---(
\begingroup
\setlength{\tabcolsep}{0pt}
\begin{tabular}{@{}c@{\hspace{0.2em}}l@{}}
  \raisebox{1mm}{\includegraphics[width=1.5cm,height=1.5cm]{galaxy_logo@[logo]@.png}} &
  \shortstack[l]{{\@[font]@\Huge Symmetra}\\[-0.35ex]{\@[font]@\Huge Capital}}
\end{tabular}
\endgroup
)---",.open="@[",.close="]@"))
}
@

